<build
    xmlns:python="http://bitten.edgewall.org/tools/python"
    xmlns:svn="http://bitten.edgewall.org/tools/svn"
    xmlns:c="http://bitten.edgewall.org/tools/c"
    xmlns:sh="http://bitten.edgewall.org/tools/sh"
    >

    <step id="checkout"> 
        <svn:checkout url="file:///${svn:repo}" path="trunk/southtut" /> 
    </step>

    <step id="bootstrap"> 
        <python:exec file="bootstrap.py" args="-d" />
    </step>

    <step id="buildout"> 
        <sh:exec executable="bin/buildout" args="${recipe-xml-in:config}"/> 
    </step>

    <step id="syncdb"> 
        <sh:exec executable="${buildout:directory}/ci-syncdb.sh" />
    </step>

</build>
